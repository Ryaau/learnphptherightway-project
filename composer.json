{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.6",
        "psr/container": "^2.0",
        "symfony/mailer": "^7.0",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.1",
        "symfony/cache": "^7.0",
        "doctrine/migrations": "^3.7.4",
        "illuminate/database": "^11",
        "illuminate/events": "^11",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/var-dumper": "^7.0",
        "symfony/http-foundation": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    }
}
